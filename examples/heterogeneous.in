# set global parameters


domain boxdims 100.0 100.0 100.0 boxorigin -50.0 -50.0 -50.0

grid_style 64 64 64 concentration

# can either use grid_setup to read in old data or create new grid data	
grid_populate constant concentration 0.2 0.2 349810
# or
#grid_populate read concentration vtkfiles/start200000_nucd_p%_100000.vti

conjugate volfrac mobility 0.01 temp 1.0 volFH 0.01 gamma 10.0 seed 491240

fixgrid floryhuggins temp 1.0 chi 3.0 volFH 0.01

dt	     1e-3

run 100000
